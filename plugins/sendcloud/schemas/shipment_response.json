                    "type": "boolean"
                },
                "insured_value": {
                    "type": "integer"
                },
                "to_service_point": {
                    "type": "integer"
                },
                "documents": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "size": {
