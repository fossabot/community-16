                    "type": "string"
                },
                "length": {
                    "type": "string"
                },
                "width": {
                    "type": "string"
                },
                "tracking_number": {
                    "type": "string"
                },
                "tracking_url": {
                    "type": "string"
                },
                "label": {
                    "type": "object",
                    "properties": {
                        "normal_printer": {
                            "type": "array"
                        },
                        "label_printer": {
                            "type": "string"
                        }
                    }
                },
                "customs_declaration": {
                    "type": "object",
                    "properties": {
                        "id": {
