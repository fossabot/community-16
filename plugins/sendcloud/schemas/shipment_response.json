{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "parcel": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "company_name": {
                    "type": "string"
                },
                "address": {
                    "type": "string"
                },
                "address_2": {
                    "type": "string"
                },
                "house_number": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "postal_code": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "telephone": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "date_created": {
                    "type": "string"
                },
                "date_updated": {
                    "type": "string"
                },
                "shipment": {
                    "type": "object",
                    "properties": {
