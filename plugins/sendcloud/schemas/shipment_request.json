{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "parcel": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "company_name": {
                    "type": "string"
                },
                "address": {
                    "type": "string"
                },
                "address_2": {
                    "type": "string"
                },
                "house_number": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "postal_code": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "telephone": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "shipment": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "integer"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "weight": {
                    "type": "string"
                },
                "height": {
                    "type": "string"
                },
                "length": {
                    "type": "string"
                },
                "width": {
                    "type": "string"
                },
                "parcel_items": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "description": {
                                "type": "string"
                            },
                            "quantity": {
                                "type": "integer"
                            },
                            "weight": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            },
                            "sku": {
                                "type": "string"
                            },
                            "hs_code": {
                                "type": "string"
                            },
                            "origin_country": {
                                "type": "string"
                            }
                        }
                    }
                },
                "customs_invoice_nr": {
                    "type": "string"
                },
                "customs_shipment_type": {
                    "type": "string"
                },
                "external_order_id": {
                    "type": "string"
                },
                "external_shipment_id": {
                    "type": "string"
                },
                "total_order_value": {
                    "type": "string"
                },
                "total_order_value_currency": {
                    "type": "string"
                },
                "is_return": {
                    "type": "boolean"
                },
                "insured_value": {
                    "type": "integer"
                },
                "to_service_point": {
                    "type": "integer"
                },
                "request_label": {
                    "type": "boolean"
                },
                "request_label_async": {
                    "type": "boolean"
                },
                "apply_shipping_rules": {
                    "type": "boolean"
                }
            },
            "required": [
                "name",
                "address",
                "city",
                "postal_code",
                "country",
                "shipment"
            ]
        }
    },
    "required": [
        "parcel"
    ]
}